aws_region = "us-east-1"
environment = "dev"
project_name = "endo"

cognito_callback_urls = [
  "https://your-cloudfront-url/dexcom/callback",
  "https://your-cloudfront-url/auth/callback"
]

cognito_logout_urls = [
  "https://your-cloudfront-url/login"
]

dexcom_client_id = "your-dexcom-client-id"
dexcom_client_secret = "your-dexcom-client-secret"
dexcom_redirect_uri = "https://your-api-gateway-url/dev/v1/dexcom/callback"
frontend_base_url = "https://your-cloudfront-url"

# Email service configuration
# Note: This email must be verified in AWS SES before emails can be sent
sender_email = "your-verified-email@example.com"